{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    // "airbnb",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "plugin:testing-library/react",
    "plugin:cypress/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint", "cypress", "testing-library"],
  "rules": {
    "comman-dangle": 0
  }
}
